{
    "extends": "next/core-web-vitals",
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "off",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off",
        "no-await-in-loop": "off",
        "no-nested-ternary": "off",
        "max-classes-per-file": "off",
        "camelcase": "off",
        "no-underscore-dangle": "off"
    }
}